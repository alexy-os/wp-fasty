{
  "name": "@wp-fasty/frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    ".workspaces/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "css": "vite build --config vite.config.ts",
    "watch": "vite build --config vite.config.ts --watch",
    "watch:all": "concurrently \"bun run semantic:watch\" \"bun run variants:watch\" \"vite\"",
    "tailwind": "bun x @tailwindcss/cli -i ./src/assets/css/import.css -o ../../wp-fasty-ym/assets/css/theme.min.css --minify",
    "semantic:css": "bun x @tailwindcss/cli -i ./src/assets/css/import.css -o ./src/assets/css/styles.css",
    "semantic:watch": "bun x @tailwindcss/cli -i ./src/assets/css/import.css -o ./src/assets/css/styles.css --watch",
    "variants:css": "bun scripts/variants-parser.ts",
    "variants:watch": "bun scripts/variants-parser.ts --watch",
    "build:css": "bun x @tailwindcss/cli -i ./src/assets/css/import.css -o ./src/assets/css/styles.css --minify --watch",
    "build:tpl": "bun scripts/templates-builder.ts",
    "slot:parse": "bun scripts/data-slot-parser.ts"
  },
  "dependencies": {
    "@babel/generator": "^7.27.1",
    "@babel/types": "^7.27.1",
    "lucide-react": "^0.503.0",
    "react": "^19.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@babel/traverse": "^7.27.1",
    "@tailwindcss/cli": "^4.1.4",
    "@tailwindcss/vite": "^4.1.5",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "glob": "^11.0.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.4"
  }
}