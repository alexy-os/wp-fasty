{
  "name": "@uikits/headless",
  "version": "0.0.1",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./button": {
      "types": "./button/index.ts",
      "import": "./button/index.ts"
    },
    "./slot": {
      "types": "./slot/index.ts",
      "import": "./slot/index.ts"
    }
  },
  "files": [
    "button",
    "slot",
    "*.ts",
    "*.tsx"
  ],
  "dependencies": {
    "@vanilla-extract/css": "^1.0.0",
    "tailwind-merge": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}